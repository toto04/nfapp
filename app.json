{
  "expo": {
    "name": "nfapp",
    "slug": "nfapp",
    "scheme": "nfapp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon/full.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.tommasomorganti.nfapp",
      "supportsTablet": false
    },
    "android": {
      "versionCode": 5,
      "package": "com.tommasomorganti.nfapp",
      "icon":"./assets/icon/rounded.png",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/adaptive/foreground.png",
        "backgroundImage": "./assets/icon/adaptive/background.png"
      }
    }
  }
}