{
  "expo": {
    "name": "nfapp",
    "slug": "nfapp",
    "scheme": "nfapp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.tommasomorganti.nfapp",
      "supportsTablet": false
    },
    "android": {
      "package": "com.tommasomorganti.nfapp"
    }
  }
}
